ue === true ? 'row' : 'row active'}>\n          <div>\n              <button type=\"button\" className=\"open-sidebar\" onClick={() => setSideBarIsOpen(true)}>\n                <i className=\"fa fa-bars\"></i>\n              </button>\n              <Link className=\"brand\" to=\"/\">OSO</Link>\n          </div>\n          <div>\n            <Route render={({history}) => <SearchBox history={history}></SearchBox>}></Route>\n          </div>\n          <div className=\"nav-links\">\n              <Link to=\"/cart\">Cart\n              {cartItems.length > 0 && (\n                <span className=\"badge\">( {cartItems.length} ) </span>\n              )}\n              </Link>\n\n            {\n              userInfo ? (\n                <div className=\"dropdown\">\n                  <Link to =\"#\">{userInfo.name} <i className=\"fa fa-caret-down\"></i></Link>\n                  <ul className=\"dropdown-content\"  style={{width: \"20rem\"}}>\n                    <li>\n                      <Link to=\"/profile\">Update User</Link>\n                    </li>\n                    <li>\n                      <Link to=\"/orderhistory\">Order History</Link>\n                    </li>\n                    <li>\n                      <Link to=\"#signout\" onClick={signoutHandler}>Sign Out</Link>\n                    </li>\n                  </ul>\n                </div>\n                ) : \n                (\n                  <Link to=\"/signin\">Sign In</Link>\n                )\n            }\n            { userInfo && userInfo.isSeller && (\n              <div className=\"dropdown\">\n                <Link to=\"#seller\">Seller {''} <i className=\"fa fa-caret-down\"></i></Link>\n                <ul className=\"dropdown-content\">\n                  <li>\n                    <Link to=\"/productlist/seller\">Products</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/orderlist/seller\">Orders</Link>\n                  </li>\n                </ul>\n              </div>\n            )}\n            {\n              userInfo && userInfo.isAdmin && (\n                <div className=\"dropdown\">\n                  <Link to=\"#admin\">Admin {''} <i className=\"fa fa-caret-down\"></i></Link>\n                  <ul className=\"dropdown-content\">\n                    <li>\n                      <Link to=\"/productlist\">Products</Link>\n                    </li>\n                    <li>\n                      <Link to=\"/orderlist\">Orders</Link>\n                    </li>\n                    <li>\n                      <Link to=\"/userlist\">Users</Link>\n                    </li>\n                  </ul>\n                </div>\n              )\n            }\n          </div>\n        </header>\n        <aside className={sidebarIsOpen ? 'open' : ''}>\n          <ul className=\"categories\">\n            <li style={{fontSize: \"larger\"}}>\n                Categories\n              <button onClick={() => setSideBarIsOpen(false)} className=\"close-sidebar\" type=\"button\">\n                <i className=\"fa fa-close\"></i>\n              </button>\n            </li>\n            { loadingCategories ? (<LoadingBox></LoadingBox>)\n                : errorCategories ? (<MessageBox variant=\"danger\">{errorCategories}</MessageBox>)\n                :\n                categories.map(c => (\n                  <li key={c}>\n                    <Link to={`/search/category/${c}`} onClick={() => setSideBarIsOpen(false)}>{c}</Link>\n                  </li>\n                ))\n            }\n          </ul>\n        </aside>\n        <main>\n          <Route path=\"/seller/:id\" component={SellerScreen}></Route>\n          <Route path=\"/cart/:id?\" component={CartScreen}></Route>\n          <Route path=\"/product/:id\" component={ProductScreen} exact></Route>\n          <Route path=\"/product/:id/edit\" component={ProductEditScreen} exact></Route>\n          <Route path=\"/signin\" component={SigninScreen}></Ro